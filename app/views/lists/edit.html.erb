<div class="container">
    <div class="row">
<%= form_for @list, url: list_path(@list), method: :patch do |f|  %>
  <%= f.text_field :name, placeholder:"Name" , :autofocus => true %>
  <%= f.text_field :mobile  %>
  <%= f.text_field :message, placeholder:"message"  %>
  <%= f.submit value: "Update" %>
<% end %>
</div>
</div>